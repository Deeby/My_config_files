if [[ -n $(ps aux | grep XMDate | grep -v XMDateState | grep -v grep | awk '{print $2}') ]]; then
    kill $(ps aux | grep XMDate | grep -v XMDateState | grep -v grep | awk '{print $2}') &
    echo > /tmp/haron/DO &
fi
XMTimeState &
XMNetState &
XMTrafState &
XMCPUState &
XMTempState &
XMMemState &
XMVolState &
exit 0
