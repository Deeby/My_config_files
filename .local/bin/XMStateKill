kill $(ps axu | grep XMNetSpeed                         | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMTraf     | grep -v XMTrafState   | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMCUNew                            | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMTempAll                          | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMMemAll                           | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMRomNew                           | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMUptime   | grep -v XMUptimeState | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMVolNew                           | grep -v grep | awk '{print $2}') &
kill $(ps axu | grep XMDate     | grep -v XMDateState   | grep -v grep | awk '{print $2}') &
echo > /tmp/haron/{CU,DO,MEM,MW2,NS,RU,TEMP,TR,UT,VO} &
exit 0
