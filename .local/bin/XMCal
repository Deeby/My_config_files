if [[ -n $(pidof dunst) ]]; then 
    kill $(pidof dunst)
else 
    kill $(pidof dunst) &
    cal -y | xargs -0 notify-send -i "/home/haron/.icons/nouveGnomeGray/24x24/emblems/emblem-calendar.png" -t 0
fi
exit 0
