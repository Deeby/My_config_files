if (pidof dunst >/dev/null); then 
    kill $(pidof dunst)
else 
    ps -eo pid,ppid,cmd,%mem,%cpu --sort=-%mem | head -n 5 | column -t | xargs -0 notify-send -i "/home/haron/.icons/nouveGnomeGray/24x24/devices/media-floppy.png" -t 0
fi
exit 0
