if [[ -n $(pidof dunst) ]]; then 
    kill $(pidof dunst)
else
    kill $(pidof dunst) &
    ifconfig | xargs -0 notify-send -i "/home/haron/.icons/nouveGnomeGray/24x24/devices/network-wired.png" -t 0
fi
exit 0
